{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["../../Website/*"]
    }
  },
  "include": ["../../Website/next-env.d.ts", "../../Website/**/*.ts", "../../Website/**/*.tsx", "../../Website/.next/types/**/*.ts"],
  "exclude": ["../../Website/node_modules"]
}






